  .block-validator .block-validator__plugin-submit { width: 100%; }
  .block-validator .block-validator__plugin-input-container { display: bloc=
k; }
  .block-validator .block-validator__plugin-input { width: 100%; }
  .block-validator .block-validator__plugin-submit { margin-left: 0px; marg=
in-top: var(--wp--style--block-gap); }
